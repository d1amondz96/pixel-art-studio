{
  "version": 2,
  "builds": [
    { "src": "website/**", "use": "@vercel/static" },
    { "src": "pixel_art_studio_new.exe", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/pixel_art_studio_new.exe", "destination": "/pixel_art_studio_new.exe" },
    { "source": "/(.*)", "destination": "/website/$1" }
  ],
  "headers": [
    {
      "source": "/icon.ico",
      "headers": [{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }]
    },
    {
      "source": "/pixel_art_studio_new.exe",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Content-Disposition", "value": "attachment; filename=pixel_art_studio_new.exe" }
      ]
    }
  ]
}
