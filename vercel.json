{
  "version": 2,
  "builds": [{ "src": "website/**", "use": "@vercel/static" }],
  "rewrites": [{ "source": "/(.*)", "destination": "/website/$1" }],
  "headers": [
    {
      "source": "/icon.ico",
      "headers": [{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }]
    }
  ]
}
